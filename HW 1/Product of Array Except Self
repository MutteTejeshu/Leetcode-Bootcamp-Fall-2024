class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        answer = [1] * len(nums)

        first = 1
        for i in range(len(nums)):
            answer[i] *= first
            first *= nums[i]

        second =1
        for j in range(len(nums)-1, -1, -1):
            answer[j] *= second
            second *= nums[j]

        return answer
        
